// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {

	provider = "sqlite"
	url = "file:./dev.db"
}

model songs {
	id		 				Int      @id @default(autoincrement())
	title  				String
	artist 				String
	album 				String
	image 				String
	audioFile 		String
	releaseDate 	String
}